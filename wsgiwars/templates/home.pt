<metal:main use-macro="load: layout.pt" >
  <div class="span9" metal:fill-slot="content" tal:define="linkT load: subtemplates/link.pt" >
    <article tal:repeat="link links" metal:use-macro="linkT">
    </article>
  </div>

  <div metal:fill-slot="rss" class="span9">
    <a href="${request.route_path('rss')}"><i class="icon-rss"></i> RSS</a>
  </div>
</metal:main>
